---
import Layout from "../../layouts/default.astro";
import IndexList from "../../components/IndexList/Groups.astro";
import List from "../../components/IndexList/Lists/Texts.astro";
import Title from "../../components/Title/Title.astro";
const modules = import.meta.glob("./proj/*.md", { eager: true });
const media = Object.values(modules);
---

<Layout>
  <Title
    title="My Projects"
    about="Here you can read about some of my projects."
    icon="f0eb"
  />
  <IndexList media={media} groupBy={["date", "catagory"]} List={List} />
</Layout>
